  "customHead" : "<script type=\"text/javascript\" src=\"//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js\"></script>\n<style>\n  .hs_recaptcha{\n    width: 50% !important;\n  }\n  .actions{\n    display: flex;\n    justify-content: flex-end;\n  }\n  .hs-button{\n    width: 100px;\n    margin-left: 0;\n    height: 40px;\n  }\n  textarea,\n  select,\n  input[type=email],\n  input[type=text],\n  input[type=tel]{\n    width: 100% !important;\n  }\n  @media(max-width:768px){\n    .hs_recaptcha, .hs-submit{\n      width: 100%;\n    }\n    .actions{\n      justify-content: center;\n    }\n  }\n</style>\n",
  "customFooter" : "<script>\n  \n  $(document).ready(function(){\n    let width = $(window).width()\n\n    $(\"a\").removeAttr(\"href\")\n\n    $('a').click(function(){\n      topFunction()\n    })\n\n    if(width <= 768){\n      $('.folders-container').slick({\n        autoplay: false,\n        dots: false,\n        slidesToShow: 1,\n        slidesToScroll: 1,\n        arrows: true,\n        prevArrow: '<button type=\"button\" class=\"slick-prev\"><img src=\"https://f.hubspotusercontent40.net/hubfs/5292292/Digifianz/2020/Assets/Miscellaneous/chevron-dark.png\"></button>',\n        nextArrow: '<button type=\"button\" class=\"slick-next\"><img src=\"https://f.hubspotusercontent40.net/hubfs/5292292/Digifianz/2020/Assets/Miscellaneous/chevron-dark.png\"></button>',\n      }) \n    }\n    \n    window.addEventListener('message', async(event) => {\n      if(event.data.eventName === 'onFormReady') {\n        $('.legal-consent-container input').trigger('click')\n      }\n    })\n    \n  })\n  \n</script>",
